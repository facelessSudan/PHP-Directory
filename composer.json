{
    "name": "Fadhil/ai-recruitment-agent",
    "description": "AI-powered recruitment agent for resume screening",
    "type": "project",
    "require": {
	    "php": ">=7.4",
	    "vlucas/phpdotenv": "^5.4",
	    "guzzlehttp/guzzle": "^7.4",
	    "google/apiclient": "^2.12",
	    "phpmailer/phpmailer": "^6.6",
	    "spatie/pdf-to-text": "^1.52",
	    "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
	"squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
	    "App\\": "src/"
	}
    },
    "autoload": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
	"lint": "phpcs"
    }
}
